<Window x:Class="Przychodnia.ActualRegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przychodnia"
        mc:Ignorable="d"
        Title="ActualRegisterWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="patients" Margin="255,0,0,238" RenderTransformOrigin="0.5,0.5"></DataGrid>
        <Button Content="Zarejestruj nowego pacjenta" HorizontalAlignment="Left" Margin="46,316,0,0" VerticalAlignment="Top" Width="161" Click="Register"/>
        <Button Content="Usuń" HorizontalAlignment="Left" Margin="46,341,0,0" VerticalAlignment="Top" Width="161" Click="DeleteRow"/>
        <Button Content="Edytuj" HorizontalAlignment="Left" Margin="46,266,0,0" VerticalAlignment="Top" Width="161" Click="EditRow"/>
        <DataGrid x:Name="edit" Margin="255,243,0,0"></DataGrid>
        <Button Content="Zapisz zmiany" HorizontalAlignment="Left" Margin="46,291,0,0" VerticalAlignment="Top" Width="161" Click="SaveEditing"/>
        <Button Content="Powrót" HorizontalAlignment="Left" Margin="46,366,0,0" VerticalAlignment="Top" Width="161" Click="BackToMain"/>
    </Grid>

</Window>
