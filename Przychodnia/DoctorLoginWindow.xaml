<Window x:Class="Przychodnia.DoctorLoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przychodnia"
        mc:Ignorable="d"
        Title="DoctorLoginWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Karty pacjentów" HorizontalAlignment="Left" Margin="50,301,0,0" VerticalAlignment="Top" Width="135" Height="46" Click="Card_patient"/>
        <Button Content="Utwórz nową kartę" HorizontalAlignment="Left" Margin="190,301,0,0" VerticalAlignment="Top" Width="135" Height="46" Click="Create_new_card"/>
        <Button x:Name="logout" Content="Wyloguj" HorizontalAlignment="Left" Margin="647,10,0,0" VerticalAlignment="Top" Width="135" Height="46" Click="Logout"/>
        <Button Content="Zarządzaj chorobami" HorizontalAlignment="Left" Margin="330,301,0,0" VerticalAlignment="Top" Width="135" Height="46" Click="MenageSicks"/>
    </Grid>
</Window>
